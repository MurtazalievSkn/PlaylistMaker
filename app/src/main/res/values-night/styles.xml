<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SettingsBackground">
        <item name="android:background">@color/black</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/black</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="EditTextButtonStyle">
        <item name="tint">@color/icon_black_color</item>
        <item name="backgroundTint">@color/black</item>
    </style>

    <style name="ButtonBackground">
        <item name="android:background">@color/black</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="SettingsActivityTitle">
        <item name="android:textColor">@color/white</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="TextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:drawableTint">@color/icon_black_color</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColorHint">@color/icon_black_color</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="MessageError">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ButtonUpdate">
        <item name="android:textColor">@color/icon_black_color</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>
</resources>